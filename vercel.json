{
    "version": 2,
    "builds": [
        {
            "src": "app/package.json",
            "use": "@vercel/next"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app/$1"
        }
    ],
    "env": {
        "DATABASE_URL": "@database-url",
        "ANTHROPIC_API_KEY": "@anthropic-api-key"
    }
}